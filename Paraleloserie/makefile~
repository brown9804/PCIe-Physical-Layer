all: clean  autosyn sed ejecutable tabla
	echo Simulando valores...
	gtkwave ptos.vcd
	echo listo!

ejecutable:
	echo generando ejecutable...
	iverilog tb.v -o exe.o
	echo listo!
tabla:
	echo Creando tabla de datas...
	vvp exe.o
	echo listo!
clean:
	rm -f *.o
	rm -f *.vcd
	rm -f paralelltoserial_syn.v
	rm -f paralelltoserial_struct.v

run:
	echo Simulando valores...
	gtkwave ptos.vcd
	echo listo!
sed:
	sed -e ' s/paralelltoserial/serieparalelo_struct/' ./paralelltoserial_syn.v >> paralelltoserial_struct.v
	rm -f paralleltoserial_syn.v
autosyn: 
	yosys ./Autosyn.ys
